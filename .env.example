# Ponder RPC URL for Base chain
PONDER_RPC_URL_8453=https://your-base-rpc-url

# StoriesInMotionV5 contract address
CONTRACT_ADDRESS=0x...

# Block number to start indexing from
START_BLOCK=0

# API key for authenticating with the backend service
INDEXER_API_KEY=your-secure-api-key

# Database URL (optional - Ponder will create a local SQLite database by default)
# DATABASE_URL=postgresql://user:password@localhost:5432/ponder

# Log level (optional)
# PONDER_LOG_LEVEL=info